<div id="form">
		  <p class="validateTips"><small>All fields are required</small></p>
		 
		  <form>
			  <fieldset>
				<div class="row-fluid">
						<input type="text" name="name" id="name" placeholder="Name..." class="" />
				</div>
				<div class="row-fluid">     
					<input type="email" name="email" id="email" placeholder="Email..." class="" />
				</div>
				<div class="row-fluid">    
				    <input type="text" name="subject" id="subject" placeholder="Subject..." class="" />
				</div>
				<div class="row-fluid"> 
				    <textarea name="message" id="message" rows="10" placeholder="Message..." class="message"></textarea>
				</div>
			  </fieldset>
		  </form>
	</div>