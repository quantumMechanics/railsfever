<div id="form">
		  <!--p class="validateTips"><small>All fields are required</small></p-->
		 
		  <form method="post">
			  <fieldset>
				<div class="row-fluid">
						<input type="text" name="first_name" id="first-name" placeholder="First Name" class="" />
				</div>
				<div class="row-fluid">
						<input type="text" name="last_name" id="last-name" placeholder="Last Name" class="" />
				</div>
				<div class="row-fluid">     
					<input type="email" name="email" id="email" placeholder="Email Address" class="" />
				</div>
				<div class="row-fluid">
						<input type="tel" name="tel" id="tel" placeholder="Telephone Number" class="" />
				</div>
				<div class="row-fluid">    
				    <input type="text" name="subject" id="subject" placeholder="Subject" class="" />
				</div>
				<div class="row-fluid"> 
				    <textarea name="body" id="message" rows="10" placeholder="Please describe your project" class="message"></textarea>
				</div>
				<div class="row-fluid">
					<button class="pull-right button">Send</button>
				</div>
			  </fieldset>
		  </form>
	</div>